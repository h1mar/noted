{
	"expo": {
		"name": "noted",
		"slug": "noted",
		"version": "1.0.0",
		"orientation": "portrait",
		"icon": "./assets/ic_launcher_xxxhdpi.png",
		"backgroundColor": "#000000",
		"updates": {
			"fallbackToCacheTimeout": 0
		},
		"splash": {
			"image": "./assets/splash.png",
			"backgroundColor": "#000000"
		},
		"assetBundlePatterns": ["**/*"],
		"ios": {
			"supportsTablet": true
		},
		"web": {
			"favicon": "./assets/ic_launcher_mdpi.png"
		},
		"androidStatusBar": {
			"backgroundColor": "#000000",
			"translucent": false,
			"barStyle": "light-content"
		},
		"androidNavigationBar": {
			"backgroundColor": "#000000",
			"barStyle": "light-content"
		},
		"android": {
			"package": "dev.himar.noted",
			"useNextNotificationsApi": true
		},
		"notification": {
			"icon": "./assets/ic_launcher_xhdpi_notif_circle_reverse.png",
			"color": "#ffffff"
		}
	}
}
